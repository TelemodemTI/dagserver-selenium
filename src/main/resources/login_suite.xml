<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Login">
    <test name="loginOkTest">
    	<parameter name="url" value="http://localhost:8080/dagserver/cli/index.html"/>  
    	<parameter name="username" value="dagserver"/>  
    	<parameter name="pwd" value="dagserver"/>  
        <classes>
            <class name="dagserver.test.LoginTest">
                <methods>
                    <include name="loginOk" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="logoutOkTest">
    	<parameter name="url" value="http://localhost:8080/dagserver/cli/index.html"/>  
    	<parameter name="username" value="dagserver"/>  
    	<parameter name="pwd" value="dagserver"/>  
        <classes>
            <class name="dagserver.test.LoginTest">
                <methods>
                    <include name="logoutOk" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="loginNOkTest">
    	<parameter name="url" value="http://localhost:8080/dagserver/cli/index.html"/>  
    	<parameter name="username" value="dagserver1"/>  
    	<parameter name="pwd" value="dagserver1"/>  
        <classes>
            <class name="dagserver.test.LoginTest">
                <methods>
                    <include name="loginNOK" />
                </methods>
            </class>
        </classes>
    </test>
</suite>